<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CNToolbar">
        <attr name="rightButtonIcon"
              format="reference"></attr>
        <attr name="isShowSearchView"
              format="boolean"></attr>
        <attr name="rightButtonText"
              format="string"></attr>
    </declare-styleable>

    <declare-styleable name="NumberAddSubView">
